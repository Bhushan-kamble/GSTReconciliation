  <div class="pageheader">
                <h1>New Charted Accountant</h1>
                <div class="breadcrumb-wrapper hidden-xs">
                    <span class="label">You are here:</span>
                    <ol class="breadcrumb">
                        <li><a href="/">Dashboard</a>
                        </li>
                        <li>Forms</li>
                        <li class="active">New Charted Accountant</li>
                    </ol>
                </div>
            </div>
            <section id="main-content" class="animated fadeInUp">
                <div class="row">
                    <div class="col-md-12">
                        <div class="panel panel-default">
                            <div class="panel-heading">
                                <h3 class="panel-title">New Charted Accountant</h3>
                                <div class="actions pull-right">
                                 <i class="fa fa-expand"></i>
                                    <i class="fa fa-chevron-down"></i>
                                    <i class="fa fa-times"></i>
                                </div>
                            </div>
                            <div class="panel-body">
                           
                           <%= form_with(model: charted_accountant, local: true) do |f| %>
                            <% if charted_accountant.errors.any? %>
                            <div id="error_explanation">
                           <h2><%= pluralize(charted_accountant.errors.count, "error") %> prohibited this charted_accountant from being saved:</h2>

                            <ul>
                            <% charted_accountant.errors.full_messages.each do |message| %>
                             <li><%= message %></li>
                            <% end %>
                            </ul>
                           </div>
                           <% end %>
                                    <div class="form-group">
                                        <label class="col-sm-3 control-label">First Name</label>
                                        <div class="col-sm-12">
                                            
                                            <%= f.text_field :name,class: "form-control",placeholder: "First Name."%>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-3 control-label">Contact No</label>
                                        <div class="col-sm-12">
                                            <%= f.text_field :Phn_no, class: "form-control" ,placeholder: "No."%>
                                        </div>
                                    </div>
                                     <div class="form-group">
                                        <label class="col-sm-3 control-label">Address</label>
                                        <div class="col-sm-12">
                                            <%= f.text_field :address, class: "form-control", placeholder: "Address."%>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-3 control-label">Email</label>
                                        <div class="col-sm-12">
                                             <%= f.text_field :email, class: "form-control" ,placeholder: "Email."%>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-3 control-label">Firm Name</label>
                                        <div class="col-sm-12">
                                             <%= f.text_field :firm_name, class: "form-control", name: "input2", id: "input2", required: "" ,placeholder: "Firm Name."%>
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label class="col-sm-3 control-label">CA No</label>
                                        <div class="col-sm-12">
                                            <%= f.text_field :ca_no, class: "form-control" ,placeholder: "No."%>
                                        </div>
                                    </div>
                             
                                    <div class="panel-body">
                                        <div class="col-sm-offset-3 col-sm-12">
                                        <%= f.submit class: "btn btn-success"%>
                                        <%= link_to 'Back', charted_accountants_path,class: "btn btn-primary" %>
                                        </div>
                                    </div>
                                
                                <%end%>
                            </div>
                        </div>
                    </div>
                </div>
        </section>

       


